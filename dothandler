#!/bin/bash
#This shebang is for synax only. Do not make executable!

export BORG=$(cat ~/.local/var/myborg/current.txt)
export PATH=${PATH}:${BORG}/bin

echo [${BORG}/dothandler] $*

source borg:source:file ${HOME}/.config/myborg/activate

source borg:source:file ${BORG}/first
source borg:source:file ${BORG}/proxies/dot$1
source borg:source:file ${BORG}/last

source borg:path:clean
